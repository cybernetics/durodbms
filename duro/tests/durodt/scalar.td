VAR I INTEGER INIT 1;
PRINTLN(CAST_AS_STRING(I + 1));

VAR J SAME_TYPE_AS(I);
J:=2;
PRINTLN(CAST_AS_STRING(I + J));
VAR C CHAR INIT 'XX';
PRINTLN(C);
VAR B BOOLEAN;
PRINTLN(B);

-- Synonyms
VAR B2 BOOL;
VAR I2 INT;
VAR F FLOAT;
VAR F2 RATIONAL;
VAR F3 RAT;

TRY
    VAR I INT;
    VAR I INT;
CATCH ERR ELEMENT_EXISTS_ERROR;
    PRINTLN('error caught');
END TRY;
